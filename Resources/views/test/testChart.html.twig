    <script src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('holder/holder.min.js') }}"></script>
    <script src="{{ asset('bundles/chartjs/js/Chart.min.js') }}"></script>
