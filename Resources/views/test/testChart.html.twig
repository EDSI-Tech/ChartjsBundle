    <!-- Custom styles for this template -->
    {% block css %}
    {% endblock %}
</head>
<body>
<h1>{{ title }}</h1>
{{ chartjs_canvas('myPieChart',grafica.width,grafica.height,grafica) }}
{% block javascript %}
